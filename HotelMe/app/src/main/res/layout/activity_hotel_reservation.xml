<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gatech.hotelme.Controller.HotelReservationActivity">
    <ImageView
        android:id="@+id/imageView4"
        android:scaleType="fitXY"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/black_background"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"/>
    <ImageButton
    android:id="@+id/imageButton"
    android:layout_width="375dp"
    android:layout_height="137dp"
        android:background="@drawable/marriott"
    tools:layout_editor_absoluteX="2dp"
    tools:layout_editor_absoluteY="-2dp"
    android:layout_alignParentTop="true"
    android:layout_alignParentStart="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:id="@+id/hotel_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageButton"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:fontFamily="sans-serif-condensed"
        android:text="hotel name here"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="24sp"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="144dp"/>

    <TextView
        android:id="@+id/check_out_date_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="sans-serif-condensed"
        android:text="Check out date"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="16sp"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="235dp"
        android:layout_marginTop="22dp"
        android:layout_below="@+id/check_in_date_text"
        android:layout_alignStart="@+id/check_in_date_text"/>

    <TextView
        android:id="@+id/check_in_date_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Check In Date: "
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="16sp"
        android:layout_below="@+id/hotel_name"
        android:layout_toStartOf="@+id/hotel_name"/>

    <TextView
        android:id="@+id/check_out_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/check_out_date_text"
        android:layout_alignBottom="@+id/check_out_date_text"
        android:layout_marginStart="21dp"
        android:layout_toEndOf="@+id/check_out_time_text"
        android:ems="10"
        android:inputType="date"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/room_info_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="23dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Room Info:"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="16sp"
        android:layout_below="@+id/check_out_time_text"
        android:layout_alignStart="@+id/res_num"/>

    <TextView
        android:id="@+id/room_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/room_info_text"
        android:layout_alignEnd="@+id/check_out_date"
        android:layout_alignStart="@+id/check_in_time"
        android:layout_toEndOf="@+id/check_out_date_text"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/res_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="22dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Reservation Number:"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="16sp"
        android:layout_below="@+id/room_info_text"
        android:layout_alignStart="@+id/check_in_time_text"/>

    <TextView
        android:id="@+id/check_in_time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="26dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Check In time: "
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="16sp"
        android:layout_below="@+id/check_out_date_text"
        android:layout_alignEnd="@+id/check_out_date_text"/>

    <TextView
        android:id="@+id/check_out_time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="21dp"
        android:fontFamily="sans-serif-condensed"
        android:text="Check out time:"
        android:textColor="@color/common_google_signin_btn_text_dark_default"
        android:textSize="16sp"
        android:layout_below="@+id/check_in_time_text"
        android:layout_alignStart="@+id/check_out_date_text"/>

    <TextView
        android:id="@+id/check_in_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/check_in_time_text"
        android:layout_alignEnd="@+id/check_out_date"
        android:layout_alignStart="@+id/check_out_date"
        android:layout_alignTop="@+id/check_in_time_text"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/check_out_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/room_info"
        android:layout_alignStart="@+id/check_in_time"
        android:layout_alignTop="@+id/check_out_time_text"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/check_in_date"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/check_in_date_text"
        android:layout_alignBottom="@+id/check_in_date_text"
        android:layout_alignEnd="@+id/check_out_date"
        android:layout_alignStart="@+id/check_out_date"
        android:textSize="16sp"/>

    <TextView
        android:id="@+id/reservation_number"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/room_info"
        android:layout_alignTop="@+id/res_num"
        android:layout_toEndOf="@+id/res_num"
        android:textSize="16sp"/>

</RelativeLayout>
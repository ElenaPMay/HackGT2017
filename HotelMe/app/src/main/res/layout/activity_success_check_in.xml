<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gatech.hotelme.Controller.SuccessCheckInActivity">

    <TextView
        android:id="@+id/success_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Success!"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="36sp"
        tools:layout_editor_absoluteX="120dp"
        tools:layout_editor_absoluteY="82dp" />

    <TextView
        android:id="@+id/success_text2"
        android:layout_width="294dp"
        android:layout_height="36dp"
        android:text="You have successfully checked in to your hotel"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="149dp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="173dp"
        android:layout_height="141dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="106dp"
        tools:layout_editor_absoluteY="222dp" />

    <Button
        android:id="@+id/go_to_hotel_key_screen"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="GET DIGITAL ROOM KEY"
        tools:layout_editor_absoluteX="101dp"
        tools:layout_editor_absoluteY="398dp" />

</android.support.constraint.ConstraintLayout>

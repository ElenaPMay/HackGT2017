<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="gatech.hotelme.Controller.ServicesAndAmenities">

    <Button
        android:id="@+id/hotel_info"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onHotelInfoPressed"
        android:text="hotel info"
        tools:layout_editor_absoluteX="141dp"
        tools:layout_editor_absoluteY="183dp" />

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="375dp"
        android:layout_height="139dp"
        android:background="@drawable/marriott"
        tools:layout_editor_absoluteX="4dp"
        tools:layout_editor_absoluteY="2dp" />

    <Button
        android:id="@+id/room_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:onClick="onRoomServicePressed"
        android:text="Room Service"
        tools:layout_editor_absoluteX="131dp"
        tools:layout_editor_absoluteY="251dp" />

    <Button
        android:id="@+id/cleaning_service"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cleaning Service"
        tools:layout_editor_absoluteX="117dp"
        tools:layout_editor_absoluteY="323dp" />

    <Button
        android:id="@+id/hotel_resturant"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="hotel resturant"
        tools:layout_editor_absoluteX="116dp"
        tools:layout_editor_absoluteY="394dp" />
</android.support.constraint.ConstraintLayout>
